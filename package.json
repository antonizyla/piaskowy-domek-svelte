{
	"name": "piaskowy-domek-sveltekit",
	"version": "0.0.1",
	"private": true,
	"url": "https://piaskowydomek.com",
	"scripts": {
		"postbuild": "node --experimental-json-modules ./generate-sitemap.js",
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"typesafe-i18n": "typesafe-i18n"
	},
	"devDependencies": {
		"@rodneylab/sveltekit-components": "^1.0.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.46",
		"@sveltejs/kit": "next",
		"@types/sharp": "^0.31.0",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.7",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"fast-glob": "^3.2.12",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"sass": "^1.55.0",
		"sharp": "^0.31.1",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.5",
		"tslib": "^2.3.1",
		"typescript": "^4.9.3",
		"vanilla-lazyload": "^17.8.3",
		"vite": "^3.1.0",
		"vite-imagetools": "^3.4.0-next.7",
		"xmlbuilder2": "^3.0.2"
	},
	"type": "module",
	"peerDependencies": {
		"leaflet": "^1.9.2"
	},
	"dependencies": {
		"@sveltejs/adapter-node": "^1.0.0-next.100",
		"@sveltejs/adapter-vercel": "^1.0.0-next.84",
		"typesafe-i18n": "^5.16.3"
	}
}
