{
	"name": "piaskowy-domek-sveltekit",
	"version": "0.0.1",
	"private": true,
	"url": "https://piaskowydomek.com",
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"generateSitemap": "node --experimental-json-modules ./generate-sitemap.js",
		"dev": "vite dev --host",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"typesafe-i18n": "typesafe-i18n"
	},
	"devDependencies": {
		"@rodneylab/sveltekit-components": "^1.0.3",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.6",
		"@sveltejs/kit": "next",
		"@types/sharp": "^0.31.1",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.15",
		"eslint": "^8.47.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"fast-glob": "^3.3.1",
		"postcss": "^8.4.28",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"sass": "^1.66.1",
		"sharp": "^0.31.3",
		"svelte": "^3.59.2",
		"svelte-check": "^2.10.3",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^4.9.5",
		"vanilla-lazyload": "^17.8.4",
		"vite": "^3.2.7",
		"vite-imagetools": "^3.9.0",
		"xmlbuilder2": "^3.1.1"
	},
	"type": "module",
	"peerDependencies": {
		"leaflet": "^1.9.2"
	},
	"dependencies": {
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/adapter-vercel": "^1.0.6",
		"svelte-icons": "^2.1.0",
		"typesafe-i18n": "^5.26.1"
	}
}
